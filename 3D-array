#include<stdio.h>
int main()
{
int n,i,j,a[3][3];
printf ("enter the array elements:");
for(i=0;i<3;i++)
{
for(j=0;j<3;j++)
{
printf("elements are [%d],[%d] :",i,j );
scanf("%d",&a[i][j]);
}
}
printf("3d array");
for(i=0;i<3;i++)
{
printf("\n");
for(j=0;j<3;j++)
printf("%d\t",a[i][j]);
}
printf("\n");
}
