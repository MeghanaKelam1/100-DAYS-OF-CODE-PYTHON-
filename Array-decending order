#include<stdio.h>
int main()
{
int a[10],temp,i,j,n;
printf("enter the size:");
scanf("%d",&n);
printf("enter array elements:");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
for(i=0;i<n;i++)
{
for(j=i+1;j<n;j++)
{
if(a[j] > a[i])
{
temp = a[i];
a[i] = a[j];
a[j] = temp;
}
}
}

printf("the elements in decending order is:\n");
for(i=0;i<n;i++)
printf("%d",a[i]);
